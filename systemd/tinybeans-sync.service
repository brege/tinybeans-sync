[Unit]
Description=Sync Tinybeans photos
After=network.target

[Service]
Type=oneshot
User=tinybeans
Group=tinybeans
WorkingDirectory=/opt/tinybeans
ExecStart=/usr/bin/python3 /opt/tinybeans/date_handler.py --from-last-date
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target
